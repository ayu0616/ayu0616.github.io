{
    "name": "new-app",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "bunx --bun react-router dev",
        "build": "react-router build",
        "start": "bunx tsx main.ts",
        "lint": "biome check --write .",
        "cp:img": "bunx ts-node --project tsconfig.local.json src/scripts/copyImg.ts",
        "prepare": "node --eval 'process.exit(process.env.NODE_ENV === `production` ? 0 : 1)' || husky",
        "typecheck": "react-router typegen && tsc",
        "lint-staged": "lint-staged --config .lintstagedrc.js",
        "codegen": "bunx tsx app/scripts/codegen.ts",
        "push-content": "rm blog-contents.zip && python blog-contents/update_page_info.py && zip -r blog-contents.zip blog-contents && cd terraform && terraform apply -var-file vars.tfvars -auto-approve && cd ..",
        "update-cors-config": "gcloud storage buckets update gs://hassaku-blog-contents --cors-file=gcloud-config/cors.json"
    },
    "dependencies": {
        "@ffmpeg/ffmpeg": "^0.12.15",
        "@ffmpeg/util": "^0.12.2",
        "@hono/zod-validator": "^0.4.3",
        "@radix-ui/react-label": "^2.1.2",
        "@radix-ui/react-slot": "^1.1.2",
        "@react-router/node": "^7.4.0",
        "@react-router/serve": "^7.4.0",
        "@tailwindcss/vite": "^4.0.15",
        "@tanstack/react-query": "^5.69.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.13",
        "hono": "^4.7.5",
        "hono-react-router-adapter": "^0.6.3",
        "isbot": "^5.1.25",
        "jotai": "^2.12.2",
        "js-yaml": "^4.1.0",
        "katex": "^0.16.21",
        "lucide-react": "^0.483.0",
        "next-themes": "^0.4.6",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-icons": "^5.5.0",
        "react-markdown": "^10.1.0",
        "react-router": "^7.4.0",
        "recharts": "^2.15.1",
        "rehype-katex": "^7.0.1",
        "rehype-raw": "^7.0.0",
        "remark-breaks": "^4.0.0",
        "remark-gfm": "^4.0.1",
        "remark-math": "^6.0.0",
        "satori": "^0.12.1",
        "sharp": "^0.33.5",
        "sonner": "^2.0.1",
        "tailwind-merge": "^3.0.2",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@react-router/dev": "^7.4.0",
        "@tailwindcss/postcss": "^4.0.15",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^22.13.13",
        "@types/react": "^19.0.12",
        "@types/react-dom": "^19.0.4",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.0",
        "postcss": "^8.5.3",
        "quicktype": "^23.0.171",
        "sass": "^1.86.0",
        "schema-dts": "^1.1.5",
        "shiki": "^3.2.1",
        "tailwindcss": "^4.0.15",
        "typescript": "^5.8.2",
        "vite": "^6.2.3",
        "vite-tsconfig-paths": "^5.1.4"
    },
    "trustedDependencies": ["sharp"]
}
