{
    "name": "new-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbo",
        "build": "pnpm cp:img && next build && next-export-optimize-images",
        "start": "pnpx serve@latest out",
        "lint": "biome check --write .",
        "cp:img": "pnpm dlx ts-node --project tsconfig.local.json src/scripts/copyImg.ts",
        "prepare": "node --eval 'process.exit(process.env.NODE_ENV === `production` ? 0 : 1)' || husky",
        "typecheck": "tsc --noEmit",
        "lint-staged": "lint-staged --config .lintstagedrc.js",
        "codegen": "pnpm dlx tsx src/scripts/codegen.ts",
        "push-content": "rm blog-contents.zip && python blog-contents/update_page_info.py && zip -r blog-contents.zip blog-contents && cd terraform && terraform apply -var-file vars.tfvars -auto-approve && cd .."
    },
    "dependencies": {
        "@next/third-parties": "14.3.0-canary.44",
        "@radix-ui/react-slot": "^1.1.0",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.11",
        "js-yaml": "^4.1.0",
        "katex": "^0.16.10",
        "lucide-react": "^0.439.0",
        "next": "14.2.3",
        "next-export-optimize-images": "^4.5.0",
        "react": "^18",
        "react-dom": "^18",
        "react-icons": "^5.2.0",
        "react-markdown": "^9.0.1",
        "react-syntax-highlighter": "^15.5.0",
        "recharts": "^2.12.6",
        "rehype-katex": "^7.0.0",
        "rehype-raw": "^7.0.0",
        "remark-breaks": "^4.0.0",
        "remark-gfm": "^4.0.0",
        "remark-math": "^6.0.0",
        "sharp": "^0.33.3",
        "tailwind-merge": "^2.3.0",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@biomejs/biome": "1.8.3",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^22.2.0",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "@types/react-syntax-highlighter": "^15.5.13",
        "autoprefixer": "^10.4.19",
        "husky": "^9.1.5",
        "lint-staged": "^15.2.9",
        "next-sitemap": "^4.2.3",
        "postcss": "^8",
        "quicktype": "^23.0.170",
        "saas": "^1.0.0",
        "sass": "^1.76.0",
        "schema-dts": "^1.1.2",
        "tailwindcss": "^3.4.1",
        "ts-node": "^10.9.2",
        "typescript": "^5"
    },
    "packageManager": "pnpm@9.3.0+sha512.ee7b93e0c2bd11409c6424f92b866f31d3ea1bef5fbe47d3c7500cdc3c9668833d2e55681ad66df5b640c61fa9dc25d546efa54d76d7f8bf54b13614ac293631"
}
