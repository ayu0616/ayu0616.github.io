{
    "name": "new-app",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "react-router dev",
        "build": "react-router build",
        "start": "react-router-serve ./build/server/index.js",
        "lint": "biome check --write .",
        "cp:img": "bunx ts-node --project tsconfig.local.json src/scripts/copyImg.ts",
        "prepare": "node --eval 'process.exit(process.env.NODE_ENV === `production` ? 0 : 1)' || husky",
        "typecheck": "react-router typegen && tsc",
        "lint-staged": "lint-staged --config .lintstagedrc.js",
        "codegen": "bunx tsx app/scripts/codegen.ts",
        "push-content": "rm blog-contents.zip && python blog-contents/update_page_info.py && zip -r blog-contents.zip blog-contents && cd terraform && terraform apply -var-file vars.tfvars -auto-approve && cd .."
    },
    "dependencies": {
        "@hono/zod-validator": "^0.4.2",
        "@radix-ui/react-slot": "^1.1.1",
        "@react-router/node": "^7.1.3",
        "@react-router/serve": "^7.1.3",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.13",
        "hono": "^4.6.17",
        "hono-react-router-adapter": "^0.6.1",
        "isbot": "^5.1.21",
        "jotai": "^2.11.0",
        "js-yaml": "^4.1.0",
        "katex": "^0.16.21",
        "lucide-react": "^0.473.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-icons": "^5.4.0",
        "react-markdown": "^9.0.3",
        "react-router": "^7.1.3",
        "react-syntax-highlighter": "^15.6.1",
        "recharts": "^2.15.0",
        "rehype-katex": "^7.0.1",
        "rehype-raw": "^7.0.0",
        "remark-breaks": "^4.0.0",
        "remark-gfm": "^4.0.0",
        "remark-math": "^6.0.0",
        "tailwind-merge": "^2.6.0",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@react-router/dev": "^7.1.3",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^22.10.7",
        "@types/react": "^19.0.7",
        "@types/react-dom": "^19.0.3",
        "@types/react-syntax-highlighter": "^15.5.13",
        "autoprefixer": "^10.4.20",
        "husky": "^9.1.7",
        "lint-staged": "^15.4.1",
        "postcss": "^8.5.1",
        "quicktype": "^23.0.170",
        "sass": "^1.83.4",
        "schema-dts": "^1.1.2",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.7.3",
        "vite": "^6.0.7",
        "vite-tsconfig-paths": "^5.1.4"
    }
}
